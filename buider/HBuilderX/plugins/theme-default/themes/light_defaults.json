			"settings": {
				"foreground": "#859900"
			}
        },
        {
            "name": "List Start",
            "scope": "beginning.punctuation.definition.list.markdown",
            "settings": {
                "foreground": "#4E3B00"
            }
        },
        {
            "name": "Todo",
            "scope": "punctuation.definition.todo.markdown",
            "settings": {
                "foreground": "#E60000"
            }
        },
