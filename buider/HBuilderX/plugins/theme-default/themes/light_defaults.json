		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
                "foreground": "#ae7a00"
			}
		},
